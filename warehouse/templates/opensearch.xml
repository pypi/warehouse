    <Image height="16" width="16" type="image/icon">{{ request.static_url('warehouse:static/dist/images/favicon.ico') }}</Image>
    <InputEncoding>UTF-8</InputEncoding>
    <OutputEncoding>UTF-8</OutputEncoding>
    <ShortName>PyPI</ShortName>
    <Url type="text/html" method="GET" template="{{ request.route_url('search') }}?q={searchTerms}"/>
</OpenSearchDescription>
