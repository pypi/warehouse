    "del": "3.0.0",
    "element-closest": "^2.0.2",
    "enter-view": "0.1.0",
    "exports-loader": "0.6.4",
    "gulp": "^4.0.0",
    "gulp-brotli": "1.2.1",
    "gulp-concat": "2.6.1",
