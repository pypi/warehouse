  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.12.1",
    "sass-lint": "1.12.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "amd": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
