{
  "repository": "pypa/warehouse",
  "license": "Apache-2.0",
  "engines": {
    "node": "8.9.1"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "clipboard": "1.7.1",
    "cookie": "0.3.1",
    "del": "3.0.0",
    "enter-view": "0.1.0",
    "exports-loader": "0.6.4",
    "font-awesome": "4.7.0",
    "gulp": "3.9.1",
    "gulp-batch": "1.0.5",
    "gulp-brotli": "1.2.1",
    "gulp-cssnano": "2.1.2",
    "gulp-gzip": "1.4.0",
    "gulp-image": "4.1.0",
    "gulp-rev-all": "0.9.7",
    "gulp-rev-napkin": "0.1.0",
    "gulp-sass": "3.1.0",
    "gulp-sequence": "0.4.6",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
    "gulp-watch": "4.3.11",
    "imports-loader": "0.7.1",
    "jquery": "3.2.1",
    "uglify-js": "3.2.1",
    "vinyl-named": "1.1.0",
    "webpack": "3.10.0",
    "webpack-stream": "4.0.0",
    "whatwg-fetch": "2.0.3",
    "youtube-iframe": "1.0.3"
  },
  "devDependencies": {
    "eslint": "4.12.1",
    "sass-lint": "1.12.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
