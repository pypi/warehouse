    "cookie": "0.4.0",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "date-fns": "1.29.0",
    "debounce": "1.2.0",
    "del": "3.0.0",
    "exports-loader": "0.6.4",
